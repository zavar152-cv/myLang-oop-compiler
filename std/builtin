//register __castToByte
byte __toByteFromInt(int i);
byte __toByteFromUint(uint ui);
byte __toByteFromLong(long l);
byte __toByteFromUlong(ulong ul);

//register __castToInt
int __toIntFromByte(byte b);
int __toIntFromUint(uint ui);
int __toIntFromLong(long l);
int __toIntFromUlong(ulong ul);

//register __castToUint
uint __toUintFromByte(byte b);
uint __toUintFromInt(int i);
uint __toUintFromLong(long l);
uint __toUintFromUlong(ulong ul);

//register __castToLong
long __toLongFromUlong(ulong ul);

//register __castToUlong
ulong __toUlongFromByte(byte b);
ulong __toUlongFromInt(int i);
ulong __toUlongFromLong(long l);

//memory allocation
void __alloc(string var, long size);

//todo
long __sizeof(string type);

//base input
byte __read();
char __readChar();

//base output
void __write(byte b);
void __writeChar(char c);