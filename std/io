int strlength(string str) {
    int len = 0;
    while (true == true) {
        if (str[len] == '\0') {
            break;
        }
        len = len + 1;
    }
    len;
}

int println() {
    __writeChar('\n');
    0;
}

int printNumber(int num) {
    // Обработка отрицательных чисел
    int temp = num;
    if (temp < 0) {
        __writeChar('-');  // Выводим знак минуса
        temp = -temp;    // Преобразуем в положительное
    }

    // Обработка числа 0
    if (temp == 0) {
        __writeChar('0');  // Выводим 0 и выходим
    } else {
        // Извлечение цифр в обратном порядке
        char[] buffer;  // Буфер для хранения цифр числа
        __alloc("buffer", 10);
        int index = 0;

        while (temp > 0) {
            buffer[index] = (temp % 10) || 0x30;  // Сохраняем символ цифры
            index = index + 1;                // Явное увеличение индекса
            temp = temp / 10;                   // Переходим к следующей цифре
        }

        // Вывод цифр в правильном порядке
        index = index - 1;  // Начинаем с последнего сохранённого индекса
        while (index != -1) {
            __writeChar(buffer[index]);
            index = index - 1;  // Явное уменьшение индекса
        }
    }
    0;
}