int main(string[] args) {
    int a;

    char c = 'i';

    
    // printlnInt(123, buffer);

    string s = "Hello";

    //printlnInt(test(), buffer);

    int[][] matrix = __allocRef(32);

    matrix[0] = __allocRef(16);
    matrix[1] = __allocRef(16);
    matrix[2] = __allocRef(16);
    matrix[3] = __allocRef(16);

    matrix[0][0] = 0;
    matrix[0][1] = 10;

    matrix[1][0] = 20;
    matrix[1][1] = 21;

    matrix[2][0] = 40;
    matrix[2][1] = 50;

    matrix[3][0] = 60;
    matrix[3][1] = 701;

    ref buffer = __allocRef(80);

    bool f = __cmpRef(buffer, 97);

    if (f == false) {
        __writeChar('m');
    }

    int b = buffer;
    printlnInt(b, buffer);
    printlnInt(matrix[0][0], buffer);
    printlnInt(matrix[0][1], buffer);
    printlnInt(matrix[1][0], buffer);
    printlnInt(matrix[1][1], buffer);
    printlnInt(matrix[2][0], buffer);
    printlnInt(matrix[2][1], buffer);
    printlnInt(matrix[3][0], buffer);
    printlnInt(matrix[3][1], buffer);
    printlnInt(matrix[3][1], buffer);
    printlnInt(matrix[3][1], buffer);
    printlnInt(matrix[3][1], buffer);
    printlnInt(matrix[3][1], buffer);

    int v = matrix[0][0];
    printlnInt(v, buffer);

    int y = -2;

    byte u = __toByteFromInt(y);

    if (u == __toByteFromInt(254)) {
        __writeChar('r');
    }

    int k = -1;

    uint k1 = __toUintFromInt(k);

    if (k1 == 4294967295) {
        __writeChar('q');
    }

    // long p = 9223372036854775807;

    char[] longBuffer = __allocRef(longBufferSize() * __toLongFromInt(8));
    // printLong(p, longBuffer);
    // println();

    long p = 9223372036854775807;

    long res = p - __toLongFromInt(3);

    if (res == 9223372036854775804) {
        printLong(res, longBuffer);
        println();
    }

    0;
}

