}

int test(int a, int b, int c, int d) {
    int r = a + b + c + d;
    r;
}

char[] retArr() {
    char[] arr2;
    __alloc("arr2", 8);

    arr2[6] = '2';
    arr2[5] = 'k';
    arr2;
}

