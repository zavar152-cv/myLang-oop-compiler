    while (i != l) {
        __writeChar(s[i]);
        i = i + 1;
    }
    
    __writeChar('\n');

    int ret = test(1, 2, 3, test(1, 1, 1, test(0, 0, 0, test(0, 0, 0, 1))));
    if (ret == 10) {
        i = 0;
        l = 4;
        while (i != l) {
            __writeChar("YES"[i]);
            i = i + 1;
        }
    } else if (ret == 9) {
        i = 0;
        l = 3;
        while (i != l) {
            __writeChar("NO"[i]);
